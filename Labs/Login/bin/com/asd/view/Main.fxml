<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="385.0" prefWidth="605.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
	<children>
		<TableView fx:id="table" layoutX="21.0" layoutY="124.0" prefHeight="238.0" prefWidth="563.0">
			<columns>
				<TableColumn fx:id="col1" prefWidth="94.0" text="Account #">
					<cellValueFactory>
						<PropertyValueFactory property="col1" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="col2" prefWidth="93.0" text="Name">
					<cellValueFactory>
						<PropertyValueFactory property="col2" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="col3" prefWidth="72.0" text="Account">
					<cellValueFactory>
						<PropertyValueFactory property="col3" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="col4" prefWidth="88.0" text="City">
					<cellValueFactory>
						<PropertyValueFactory property="col4" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="col5" prefWidth="102.0" text="Account Type">
					<cellValueFactory>
						<PropertyValueFactory property="col5" />
					</cellValueFactory>
				</TableColumn>
				<TableColumn fx:id="col6" prefWidth="113.0" text="Amount">
					<cellValueFactory>
						<PropertyValueFactory property="col6" />
					</cellValueFactory>
				</TableColumn>
			</columns>
		</TableView>
		<Button fx:id="addAccount" layoutX="21.0" layoutY="35.0" mnemonicParsing="false" onAction="#AddPersonal" prefWidth="150.0" text="Add Account" />
		<Button fx:id="company" layoutX="21.0" layoutY="76.0" mnemonicParsing="false" onAction="#AddCompany" prefWidth="150.0" text="Add Company" />
		<Button fx:id="interest" layoutX="228.0" layoutY="35.0" mnemonicParsing="false" onAction="#interest" prefWidth="150.0" text="Interest" />
		<Button fx:id="deposit" layoutX="228.0" layoutY="76.0" mnemonicParsing="false" onAction="#Deposit" prefWidth="150.0" text="Deposit" />
		<Button fx:id="withdraw" layoutX="434.0" layoutY="35.0" mnemonicParsing="false" onAction="#Withdraw" prefWidth="150.0" text="Withdraw" />
		<Button fx:id="close" layoutX="434.0" layoutY="76.0" mnemonicParsing="false" onAction="#logoutBtn" prefWidth="150.0" text="Logout" />
	</children>
</AnchorPane>
