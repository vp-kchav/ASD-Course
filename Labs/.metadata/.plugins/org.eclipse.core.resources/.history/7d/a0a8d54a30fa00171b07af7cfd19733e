package mum.edu.asd;

public class Singleton {
	private static Singleton uniqueInstance = null; 
	private int data = 0; 
	public static Singleton getInstance() {   
		if (uniqueInstance == null) {
			uniqueInstance = new Singleton();    
		}
		return uniqueInstance;
	}
	 
	 /**   * The Singleton Constructor. Note that it is private! No client can   * instantiate a Singleton object directly!   */  private Singleton() {  }  // Accessors and mutators here! 
	
}
